{
  "expo": {
    "name": "Yalla Chant",
    "slug": "yalla-chant",
    "scheme": "app.vercel.yallachant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/yalla_chants_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "assets/yalla_chants_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a2e1f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "yallachant.vercel.app",
      "associatedDomains": [
        "applinks:yallachant.vercel.app"
      ],
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "config": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/yalla_chants_logo.png",
        "backgroundColor": "#0a2e1f"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "yallachant.app",
              "pathPrefix": "/chant"
            },
            {
              "scheme": "https",
              "host": "www.yallachant.app",
              "pathPrefix": "/chant"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "config": {},
      "package": "app.vercel.yallachant",
      "jsEngine": "hermes"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/yalla-chants-favicon.png",
      "output": "single"
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8144091641472082~9855189772",
          "iosAppId": "ca-app-pub-8144091641472082~1697498150"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-asset",
      "expo-audio",
      "expo-sqlite"
    ]
  }
}
